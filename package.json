{
  "name": "tapapp",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": "14.15.0"
  },
  "dependencies": {
    "@sentry/browser": "^5.27.0",
    "axios": "^0.20.0",
    "classnames": "^2.2.6",
    "connected-react-router": "^6.6.1",
    "date-fns": "^2.16.1",
    "node-sass": "^4.14.1",
    "react": "^17.0.0",
    "react-datepicker": "^3.3.0",
    "react-dom": "^17.0.0",
    "react-redux": "^7.1.3",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.4.4",
    "react-toastify": "^6.0.9",
    "redux": "^4.0.5",
    "redux-observable": "^1.2.0",
    "reselect": "^4.0.0",
    "semantic-ui-css": "^2.4.1",
    "semantic-ui-react": "^0.88.2",
    "ts-node": "^9.0.0"
  },
  "scripts": {
    "start": "craco start",
    "build": "craco build",
    "test": "craco test",
    "test:coverage": "yarn test --coverage --watchAll=false",
    "eject": "craco eject",
    "lint": "eslint --ignore-path .gitignore --ignore-path .eslintignore --ext .js,.ts,.tsx src",
    "prettier": "prettier --ignore-path .gitignore \"src/**/*.+(js|json|ts|tsx)\"",
    "format": "yarn prettier --write",
    "format:check": "yarn prettier --list-different",
    "translate": "ts-node ./scripts/translate.script.ts && prettier --write \"src/_translations/typings/*.ts\""
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@craco/craco": "^5.7.0",
    "@semantic-ui-react/craco-less": "^1.2.1",
    "@types/node": "^14.14.2",
    "@types/react": "^16.9.53",
    "@types/react-datepicker": "^3.1.1",
    "@types/react-dom": "^16.9.8",
    "@types/react-redux": "^7.1.5",
    "@types/react-router-dom": "^5.1.6",
    "@types/redux-logger": "^3.0.7",
    "@typescript-eslint/eslint-plugin": "3.10.1",
    "@typescript-eslint/parser": "3.10.1",
    "eslint-config-prettier": "^6.14.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-sort-keys-fix": "^1.1.1",
    "eslint-plugin-typescript-sort-keys": "^1.5.0",
    "prettier": "^2.1.2",
    "redux-logger": "^3.0.6",
    "semantic-ui-less": "^2.4.1",
    "typescript": "4.0.3"
  }
}
